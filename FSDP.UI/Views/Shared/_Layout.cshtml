<!DOCTYPE html>
<!--[if IE 8 ]><html class="ie" xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!-->
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US">
<!--<![endif]-->
<head>
    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <title>Eduharvard - Multi-Concept Education & Courses HTML Template</title>

    <meta name="author" content="themesflat.com">

    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Bootstrap  -->
    @*<link rel="stylesheet" type="text/css" href="~/Content/Styles/bootstrap.css">*@
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">
    <!-- Theme Style -->
    <link rel="stylesheet" type="text/css" href="~/Content/Styles/style.css">

    <!-- Responsive -->
    <link rel="stylesheet" type="text/css" href="~/Content/Styles/responsive.css">

    <!-- Animation Style -->
    <link rel="stylesheet" type="text/css" href="~/Content/Styles/animate.css">

    <!-- Favicon and touch icons  -->
    <link href="~/Content/Images/icon/icon.png" rel="shortcut icon">
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" />
    
</head>
<body>
    <div class="boxed position_form position_style3">
        <div class="wrap-header">
            <div class="top top-style3" id="topnav">
                <div class="body-content">
                    <div class="row">
                        <div class="col-md-8">
                            <ul class="flat-information">
                                <li>Mon - Sat: 8.00A - 6:00P</li>
                                <li><span>P:</span><a href=""> 1-800-INK-WELL</a></li>
                                <li><span>E:</span><a href="">  info@nib.inc</a></li>
                            </ul>
                        </div><!-- col-md-8 -->
                        <div class="col-md-4">
                            <div class="wrap-flat">
                                @Html.Partial("_LoginPartial")
                                <ul class="flat-socials">
                                    <li><a href=""><i class="fa fa-facebook"></i></a></li>
                                    <li><a href=""><i class="fa fa-twitter"></i></a></li>
                                    <li><a href=""><i class="fa fa-google-plus"></i></a></li>
                                    <li><a href=""><i class="fa fa-pinterest"></i></a></li>
                                </ul>
                            </div><!-- wrap-flat -->
                        </div><!-- col-md-4 -->
                    </div><!-- row -->
                </div> <!-- container -->
            </div><!-- top -->
            <header id="header" class="header header-style3 clearfix">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="logo">
                                <a href="@Url.Action("Index", "Home" )"><img src="~/Content/Images/greenlogo.png" alt="image"></a>
                            </div><!-- /logo -->
                            <div class="btn-menu">
                                <span></span>
                            </div>
                            <div class="wrap-nav">
                                <nav id="mainnav" class="mainnav">
                                    <ul class="menu">
                                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                        <li>@Html.ActionLink("About", "About", "Home")</li>
                                        <li>
                                            @Html.ActionLink("Contact", "Contact", "Home")
                                        </li>
                                        <li>@Html.ActionLink("Classes", "Index", "StoreClasses")</li>
                                        @if (Request.IsAuthenticated && !User.IsInRole("Customer"))
                                        {
                                            <li id="dashboard">
                                                <a href="@Url.Action("Index","Dashboard")">Dashboard</a>
                                                <ul class="submenu">
                                                    <li>@Html.ActionLink("Courses", "Index", "Courses")</li>
                                                    <li>@Html.ActionLink("Course Completions", "Index", "CourseCompletions")</li>
                                                    <li>@Html.ActionLink("Lessons", "Index", "Lessons")</li>
                                                    <li>@Html.ActionLink("Lesson Views", "Index", "LessonViews")</li>
                                                    <li>@Html.ActionLink("Classes", "Index", "StoreClasses")</li>
                                                    <li>@Html.ActionLink("Class Dates", "Index", "ClassDates")</li>
                                                    <li>@Html.ActionLink("Class Sign Ups", "Index", "ClassSignUps")</li>
                                                    @if (Request.IsAuthenticated && User.IsInRole("Admin"))
                                                    {
                                                        <li>@Html.ActionLink("Roles", "Index", "RolesAdmin")</li>
                                                        <li>@Html.ActionLink("Users", "Index", "UsersAdmin")</li>
                                                    }
                                                </ul>
                                            </li>}
                                        

                                    </ul>
                                </nav>
                            </div><!-- /wrap-nav -->
                        </div><!-- /col-md-12 -->
                    </div><!-- /row -->
                </div><!-- container -->
            </header><!-- /header -->
        </div><!-- /header3 -->
        @RenderBody()
        <!-- Footer -->
        <footer id="footer" class="footer">
            <div class="container">
                <div class="footer-information">
                    <div class="row">
                        <div class="col-md-6 col-sm-6 info-left">
                            <div class="logo-footer">
                                <a href="index.html"><img src="~/Content/Images/goldfooter.png" alt="image"></a>
                            </div><!-- logo-footer -->
                            <ul class="flat-socials">
                                <li><a href=""><i class="fa fa-facebook"></i></a></li>
                                <li><a href=""><i class="fa fa-twitter"></i></a></li>
                                <li><a href=""><i class="fa fa-google-plus"></i></a></li>
                                <li><a href=""><i class="fa fa-pinterest"></i></a></li>
                            </ul>
                        </div><!-- col-md-6 -->
                        <div class="col-md-6 col-sm-6 info-right">
                            <ul class="flat-information">
                                <li><span>P:</span><a href=""> 1-800-INK-WELL</a></li>
                                <li><span>E:</span><a href=""> info@nib.inc</a></li>
                                <li><span>A:</span><a href=""> 9100 Quill St, Lenexa, KS 66227</a></li>
                            </ul>
                        </div><!-- col-md-6 -->
                    </div><!-- row -->
                </div><!-- footer-information -->
            </div><!-- container -->
        </footer><!-- footer -->

        <div class="bottom">
            <div class="container">
                <div class="copyright">
                    <p>Copyright © @DateTime.Now.Year Nib, Inc. | Created by HomespunDev</p>
                </div>
            </div>
        </div><!-- bottom -->

        <a class="go-top show"></a>
    </div><!-- Boxed -->
    <!-- Javascript -->
    <script type="text/javascript" src="~/Content/Scripts/jquery.min.js"></script>
    @*<script type="text/javascript" src="~/Content/Scripts/bootstrap.min.js"></script>*@
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
    <script type="text/javascript" src="~/Content/Scripts/main.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.1/Chart.bundle.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/CircularLoader-v1.3.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/circle-progress.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/countdown.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/imagesloaded.min.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/jquery.isotope.min.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/jquery.mCustomScrollbar.concat.min.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/owl.carousel.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/jquery.easing.js"></script>
    <script type="text/javascript" src="~/Content/Scripts/jquery.flexslider.js"></script>
   
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.min.js"></script>
    
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap4.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.table').DataTable();
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>